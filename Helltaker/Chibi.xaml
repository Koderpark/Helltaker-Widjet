<Window x:Class="Helltaker.Chibi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Helltaker"
        mc:Ignorable="d"
        Title="HellTaker" Height="100" Width="100"
        WindowStyle = "None"
        AllowsTransparency = "True"
        Background="Transparent"
        ShowInTaskbar="True"
        >
    <Grid MouseRightButtonDown="DeleteForm" Background="Aqua" PreviewMouseLeftButtonDown="DragForm">
        <Image x:Name="Sticker"/>
    </Grid>
</Window>
